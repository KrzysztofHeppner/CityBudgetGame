                TickFrequency="10"
                IsSnapToTickEnabled="True" />
        <Slider Margin="95,174,505,225"
                Minimum="0"
                Maximum="100"
                Value="50"
                Width="200"
                Height="51"
                TickPlacement="BottomRight"
                TickFrequency="10"
                IsSnapToTickEnabled="True" />
        <Slider Margin="95,247,505,152"
                Minimum="0"
                Maximum="100"
                Value="50"
                Width="200"
                Height="51"
                TickPlacement="BottomRight"
                TickFrequency="10"
                IsSnapToTickEnabled="True" />
